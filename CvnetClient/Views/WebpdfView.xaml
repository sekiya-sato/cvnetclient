<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CvnetClient.Views"
        xmlns:Wpf="clr-namespace:Microsoft.Web.WebView2.Wpf;assembly=Microsoft.Web.WebView2.Wpf" x:Class="CvnetClient.Views.WebpdfView"
	xmlns:vm="clr-namespace:CvnetClient.ViewModels"
        mc:Ignorable="d"
        Title="{Binding Title}" Height="700" Width="800">
	<Window.DataContext>
		<vm:WebpdfViewModel />
	</Window.DataContext>
	<Grid>
		<Wpf:WebView2 Margin="2" Source="{Binding Pdfdata}"/>
	</Grid>
</Window>
